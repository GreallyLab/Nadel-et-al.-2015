#To get intergenic space:
#bedtools "current" refers to Bedtools v2.16.2
/apps1/bedtools/current/complementBed -i RDIP1/hektracks/gaps.bed -g fakechrfile > RDIP1/hektracks/nogaps.bed
/apps1/bedtools/current/subtractBed -a RDIP1/hektracks/nogaps.bed -b RDIP1/hektracks/RefSeq.bed > RDIP1/hektracks/nogapsNoGenes.bed
/apps1/bedtools/current/subtractBed -a RDIP1/hektracks/nogapsNoGenes.bed -b RDIP1/hektracks/lncRNA.bed > RDIP1/hektracks/nogapsNoGenesnoLNCRNA.bed
/apps1/bedtools/current/subtractBed -a RDIP1/hektracks/nogapsNoGenesnoLNCRNA.bed -b RDIP1/hektracks/Prom4kb.bed > RDIP1/hektracks/nogapsNoGenesnoLNCRNAnoProm.bed
/apps1/bedtools/current/intersectBed -a SecondStrandCounts/IMR90minus10 -b RDIP1/hektracks/nogapsNoGenesnoLNCRNAnoProm.bed -wao > Imr90RDIP/Intersects/minus10/IntergenicRDIP.bed
Intergenic<-read.table("~/RDIP1/hektracks/nogapsNoGenesnoLNCRNAnoProm.bed", stringsAsFactors=F)
Intergenic<-cbind(Intergenic, Intergenic[,3]-Intergenic[,2])
> sum(Intergenic[,4])
